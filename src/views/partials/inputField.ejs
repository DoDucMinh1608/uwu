<section class="input-field">
  <label for="username">Name</label>
  <input required id="username" name="username" type="text" placeholder="Input your user name" value="<%- data.username || "" %>">
</section>
<section class="input-field">
  <label for="password">Password</label>
  <input required name="password" minlength="<%- account?.password?.minlength || 0 %>" type="password" placeholder="Enter your password" value="<%- data.password || "" %>">
</section>