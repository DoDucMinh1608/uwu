<section class="input-field">
  <label for="username">Name</label>
  <input required id="username" name="username" type="text" placeholder="Input your user name" value="<%- data?.username || "" %>">
</section>
<section class="input-field">
  <label for="password">Password</label>
  <input id="password" required name="password" type="password" placeholder="Enter your password" value="<%- data?.password || "" %>">
</section>