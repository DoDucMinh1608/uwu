<%- contentFor('title') %>
REGISTER

<%- contentFor('init') %>
<link rel="stylesheet" href=".././../css/stylesheets/account2/register.css">
<script src=".././../js/account2/register.js" type="module" defer></script>

<% const account = JSON.parse(locals.account || '{}') %>
<% const data = JSON.parse(locals.data || '{}') %>
<% const register = locals.register %>

<%- contentFor('body') %>
<% if (register) { %>
<%- include('../../partials/popup-content', {
    content:"Register Successful!!!", 
    url:'./login',
    button: "Back to Login."
}) %>
<% } %>
<a href="./login"> &lt;- Back To Login</a>
<main>
  <h1>REGISTER</h1>
  <%- include('../../partials/register-form', { account, register, data }) %>
</main>